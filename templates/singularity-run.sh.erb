#!/bin/sh

set -o errexit -o nounset

export LIBPROCESS_IP=<%= @ipaddress4 %>
exec java -Djute.maxbuffer=<%= @zookeeper_max_buffer %> -cp <%= @install_dir %>/lib/*:<%= @install_dir %>/singularity-<%= @singularity_version %>.jar com.hubspot.singularity.SingularityService server <%= @install_dir %>/singularity.yml
